{
  "name": "homebrew",
  "version": "1.0.0",
  "description": "",
  "main": "www/app.js",
  "scripts": {
    "build": "ntsc -p ./src",
    "postbuild": "browserify -t [ babelify --presets es2015 ] temp/app.js > www/js/bundle.js",
    "release": "tsc -p ./src && browserify -t [ babelify --presets es2015 ] temp/app.js > www/js/bundle.js && uglifyjs www/js/bundle.js --compress --mangle -o www/js/bundle.min.js",
    "serve": "http-server ./www -o -c-1",
    "lint": "tslint -o lint.txt src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "ntypescript": "1.201609060105.1",
    "uglify-js": "2.7.3"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-motion": "^0.4.4",
    "brauhaus-ts": "git://github.com/zyzof/brauhaus-ts.git"
  }
}
